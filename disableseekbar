@Override
public void onClick(View v) {
    // TODO Auto-generated method stub

    switch(v.getId()){

        case R.id.seekbar_A:
            seekbarA.setEnabled(true);
            seekbarB.setEnabled(false);
            break;
        case R.id.seekbarB:
            seekbarA.setEnabled(false);
            seekbarB.setEnabled(true);
            break;
    }
}
