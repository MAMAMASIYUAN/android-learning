<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.personinquiry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="员工信息查询">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PersonDetailActivity"
            android:label="您的简历" />
        <activity
            android:name=".TotalPersonActivity"
            android:label="员工名单" />
        <activity
            android:name=".InformationModifyActivity"
            android:label="修改信息"></activity>
        <activity android:name=".RegisterActivity"
            android:label="注册"></activity>
    </application>

</manifest>